<h1>Edit Duck</h1>
<form action="/ducks/<%= @duck.id %>" method="POST">
    <input type="hidden" name="_method" value="PATCH">
    Name:
    <input type="text" name="name" value="<%= @duck.name %>">
    Description:
    <input type="text" name="description" value="<%= @duck.description %>">
    Student ID:
    <input type="text" name="student_id" value="<%= @duck.student_id %>">
    <input type="submit">
</form>